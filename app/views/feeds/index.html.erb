<h1>Listing feeds</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Url</th>
    <th>Suppliers</th>
    <th>Last downloaded</th>
    <th>Download status</th>
    <th>Last run</th>
  </tr>

<% @feeds.each do |feed| %>
  <tr>
    <td><%=h feed.title %></td>
    <td><%=h feed.url %></td>
    <td><%=h feed.supplier_id %></td>
    <td><%=h feed.last_downloaded %></td>
    <td><%=h feed.download_status %></td>
    <td><%=h feed.last_run %></td>
    <td><%= link_to 'Show', feed %></td>
    <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
    <td><%= link_to 'Destroy', feed, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New feed', new_feed_path %>